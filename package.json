{
  "name": "shoreagents-dashboard-user-form-electron",
  "version": "0.1.0",
  "private": true,
  "description": "ShoreAgents Dashboard - Employee productivity and task management application",
  "author": "ShoreAgents",
  "main": "electron/main.js",
  "homepage": "./",
  "build": {
    "appId": "com.shoreagents.dashboard",
    "productName": "ShoreAgents Dashboard",
    "icon": "public/ShoreAgents-Logo-only-256.png",
    "forceCodeSigning": false,
    "publish": null,
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "public/**/*",
      "node_modules/sharp/**/*"
    ],
    "asarUnpack": [
      "node_modules/sharp/**/*",
      "public/**/*"
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "public/ShoreAgents-Logo-only-256.png"
    },
    "win": {
      "target": "nsis",
      "icon": "public/ShoreAgents-Logo-only-256.ico",
      "requestedExecutionLevel": "asInvoker"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "ShoreAgents Dashboard",
      "deleteAppDataOnUninstall": false
    },
    "linux": {
      "target": "AppImage",
      "icon": "public/ShoreAgents-Logo-only-256.png"
    }
  },
  "scripts": {
    "dev": "next dev --turbo --port 3005",
    "dev:webpack": "next dev --port 3005",
    "build": "next build",
    "start": "next start --port 3005",
    "lint": "next lint",
    "electron": "electron .",
    "dev-electron": "concurrently \"npm run dev\" \"wait-on http://localhost:3005 && cross-env NODE_ENV=development electron electron/main.js\" \"npm run socket\"",
    "start-servers": "concurrently \"npm run socket\" \"npm run dev\"",
    "start-with-electron": "concurrently \"npm run dev\" \"wait-on http://localhost:3005 && cross-env NODE_ENV=development electron electron/main.js\" \"npm run socket\"",
    "socket": "node socket-server.js",
    "dev:socket": "node socket-server.js",
    "test:shifts": "node scripts/run-shift-tests.js",
    "electron-dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3005 && cross-env NODE_ENV=development electron electron/main.js\"",
    "electron-pack": "npm run build && electron-builder --publish=never",
    "dist": "npm run build && electron-builder --publish=never"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-visually-hidden": "^1.2.3",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.53.0",
    "@tabler/icons-react": "^3.35.0",
    "@tanstack/react-query": "^5.85.6",
    "@tanstack/react-query-devtools": "^5.85.6",
    "@types/bcryptjs": "^2.4.6",
    "@types/pg": "^8.15.4",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "framer-motion": "^12.23.11",
    "lucide-react": "^0.525.0",
    "motion": "^12.23.15",
    "next": "^15.5.3",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "process": "^0.11.10",
    "react": "^19.0.0",
    "react-day-picker": "^9.10.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.62.0",
    "recharts": "^3.1.0",
    "redis": "^5.8.2",
    "sharp": "^0.34.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.7",
    "sound-play": "^1.1.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "cross-env": "^10.0.0",
    "electron": "^32.2.7",
    "electron-builder": "^24.9.1",
    "eslint": "^9.35.0",
    "eslint-config-next": "15.3.5",
    "global": "^4.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.2",
    "wait-on": "^7.2.0",
    "webpack": "^5.101.3"
  }
}
